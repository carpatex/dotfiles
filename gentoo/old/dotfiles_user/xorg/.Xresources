URxvt.depth: 32
URxvt*font: xft:Noto Sans Mono:size=12
!URxvt*background: black
URxvt*background: [95]black
URxvt*foreground: white
URxvt.scrollBar: false
URxvt.perl-ext-common: selection-to-clipboard,default,matcher,font-size
URxvt.resize-font.step: 1
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.colorUL: #4682B4
URxvt.clipboard.autocopy: true
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: true
URxvt.iso14755_52: true
URxvt.geometry: 200x200 
URxvt.termName: rxvt-unicode-256color
!URxvt.keysym.Shift-KP_Page_Up: eval:scroll_up_pages 1
!URxvt.keysym.Shift-KP_Page_Down: eval:scroll_down_pages 1
URxvt.keysym.Shift-Page_Up: eval:scroll_up_pages 1
URxvt.keysym.Shift-Page_Down: eval:scroll_down_pages 1
